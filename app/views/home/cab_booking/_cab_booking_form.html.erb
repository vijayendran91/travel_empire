<div id="#cabBookingForm" class="cabBookingForm mb-5">
  <div class="col">
    <%= render :partial => "home/cab_booking/locations.html.erb"%>
  </div>
  <%= render :partial => "home/cab_booking/personal_details.html.erb" %>
 </div>
<script>
  $(document).ready(function(){
    $(".custom-file-input").on("change", function() {
      var fileName = $(this).val().split("\\").pop();
      $(this).siblings(".custom-file-label").addClass("selected").html(fileName);
    });
  });
</script>
