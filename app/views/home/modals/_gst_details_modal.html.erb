<div class="modal fade" id="gstModal" tabindex="-1" role="dialog" aria-labelledby="exampleModalLongTitle" aria-hidden="true">
  <div class="modal-dialog" role="document">
    <div class="modal-content">
      <div class="modal-header">
          <h5 class="modal-title" id="exampleModalLongTitle">GST Details</h5>
        <button type="button" class="close" data-dismiss="modal" aria-label="Close">
          <span aria-hidden="true">&times;</span>
        </button>
      </div>
      <div class="modal-body">
          <div class="modal-body-text">
          </div>
          <%=f.hidden_field :gst_lg_nm, :class=> 'gst_lg_nm'%>
          <%=f.hidden_field :gst_full_addr, :class=>'gst_full_addr'%>
      </div>
      <div class="modal-footer">
        <button type="button" class="btn green-btn" data-dismiss="modal">Close</button>
      </div>
    </div>
  </div>
</div>
